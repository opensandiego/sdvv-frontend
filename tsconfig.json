{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "bundler",
    "paths": {
      "lib-ui-charts": [
        "dist/lib-ui-charts/lib-ui-charts",
        "dist/lib-ui-charts"
      ],
      "lib-ui-components": [
        "dist/lib-ui-components/lib-ui-components",
        "dist/lib-ui-components"
      ]
    },
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "resolveJsonModule": true,
    "useDefineForClassFields": false,
    "skipLibCheck": true
  }
}
