    "cy:firefox": "cypress run --headless --record -b firefox -q -s cypress/integration/sdvv-frontend_spec.js",
    "cy:e2e": "npm-run-all e2e:chrome e2e:firefox",
    "prod": "ng build --prod"
  },
  "private": true,
