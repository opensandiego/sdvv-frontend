  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "fb:serve": "firebase emulators:start",
    "e2e:serve": "npm start & wait-on http://localhost -p 4200",
    "cy:open": "cypress open",
