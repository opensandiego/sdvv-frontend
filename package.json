    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^16.2.3",
    "postcss": "^8.2.15",
    "storybook": "^7.4.6",
    "storybook-addon-angular-router": "^1.9.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
